{
    "name": "tabinator",
    "displayName": "tabinator",
    "publisher": "9crusher",
    "repository": {"type": "git", "url": "https://github.com/9crusher/tabinator.git" },
    "description": "Quickly convert tabs to spaces in all files of you project",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:tabinator.tabsToSpaces",
        "onCommand:tabinator.spacesToTabs"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.tabsToSpaces",
                "title": "Tabs To Spaces"
            },
            {
                "command": "extension.spacesToTabs",
                "title": "Spaces To Tabs"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.39.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode-test": "^1.0.2"
    },
    "dependencies": {
        "file-uri-to-path": "^2.0.0",
        "linked-list-typescript": "^1.0.15"
    }
}
